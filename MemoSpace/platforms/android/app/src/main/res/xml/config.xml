<?xml version='1.0' encoding='utf-8'?>
<widget id="com.MemoSpace" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>MemoSpace</name>
    <description>
        MemoSpace is a memory game that helps the brain practice short-term memory. 
    </description>
    <author email="" href="https://github.com/S4ntrg/MemoSpace">
        Johan Leon - programmer - johanleon14
        Santiago Roncancio - programmer - ss2108
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="mdpi" height="57" platform="android" src="www/img/icon.png" width="57" />
    <icon density="mdpi" height="57" platform="ios" src="www/img/icon.png" width="57" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
